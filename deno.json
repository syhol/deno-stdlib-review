{
  "tasks": {
    "run": "deno run -A ./src/main.ts",
    "lume": "echo \"import 'lume/cli.ts'\" | deno run --unstable -A -",
    "site:cache": "deno cache https://deno.land/x/lume@v1.19.0/cli.ts",
    "site:build": "deno task lume",
    "site:serve": "deno task lume -s"
  },
  "imports": {
    "lume/": "https://deno.land/x/lume@v1.19.0/",
    "std/": "https://deno.land/std@0.203.0/"
  }
}